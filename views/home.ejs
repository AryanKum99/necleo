                    </div>
                </div>
            </div>
            <div class="column-2">
                <div class="div-18">
                    <div class="div-19">
                        <div class="div-20">
                            <div class="div-21">
                                <div class="div-22">
                                    <div class="div-23">
                                        <div class="div-24">Free Trial</div>
                                        <div class="div-25">|</div>
                                        <div class="div-26">2days left</div>
                                    </div>
                                    <div class="div-27">Extend free trail</div>
                                </div>
                                <img loading="lazy" srcset="..." class="img-11" />
                            </div>
                            <img loading="lazy"
                                src="https://cdn.builder.io/api/v1/image/assets/TEMP/666984cba0c569d74419ba8371f82c7b2ae439d1dd1c99370a945fae7f0b5630?"
                                class="img-12" />
                        </div>
                    </div>
                    <div class="div-28" style="display: flex; flex-wrap: nowrap; ">
                        <div class="div-29">My Projects</div>
                        <div class="div-30">
                            <div class="div-31">
                                <img loading="lazy"
                                    src="https://cdn.builder.io/api/v1/image/assets/TEMP/f2474a0c86536f4ebf436679c05538a6d31a60c067eacf628c9ec174161ce5dd?"
                                    class="img-13" />
                            </div>
                            <div class="div-32"><a href="/api/new">Create a new project</a></div>
                            <div class="div-33">
                                or try a
                                <span style="color: rgba(250, 120, 47, 1)">sample project</span>
                            </div>
                        </div>

                        <div class="container">
                            <div class="div-28">
                                <% resp.forEach(response=> {%>
                                    <% var url=response.imageUrl ; var newUrl=url.replace("/5000/3333", "/360/184" ); %>
                                        <div class="box" style="background-image: url(' <%=newUrl %>');">
                                        </div>
                                        <div style="margin-bottom: 20px; font-size: medium ;">Author: <%=
                                                response.authorName%>
                                        </div>
                                        <form action="/api/edit/<%= response._id %>"><button
                                                class="btn btn-primary">Edit</button></form>
                                        <form class="d-inline" action="/api/<%=response._id%>?_method=DELETE"
                                            method="POST">
                                            <button style="margin-bottom: 30px;" class="btn btn-danger">Delete</button>
                                        </form>
                                        <%}); %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

</body>

</html>