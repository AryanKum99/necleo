    <style>
        .container {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-around;
            align-items: flex-start;
            gap: 20px;
            /* Adjust the gap as needed */
        }

        .box {
            width: 360px;
            height: 184px;
            background-color: #fff;
            color: #fff;
            text-align: center;
            line-height: 100px;
            margin: 10px;
            border-radius: 10px;
        }
    </style>
</head>

<body>
    <div class="div">
        <div class="div-2">
            <div class="column">
                <div class="div-3">
                    <img loading="lazy"
                        src="https://cdn.builder.io/api/v1/image/assets/TEMP/d2dd8abc6ca404a6f290b72fabd359fe75121e03c833e16d2002b8e1d2bcf676?"
